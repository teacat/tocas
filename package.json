{
    "name"       : "vuepack",
    "version"    : "0.0.0",
    "description": "A modern Vue.js starter which uses Vue 2, Vuex, Vue-router and Webpack 2.",
    "main"       : "client/index.js",
    "scripts": 
    {
        "test" : "echo wow, such TeaMeow, very Spring!",
        "build": "rm -rf assets && rm -f index.html && rm -f 404.html && rm -f 200.html && webpack --config build/webpack.prod.js",
        "dev"  : "node build/server.js",
        "gh"   : "npm run build && gh-pages -d dist"
    },
    "repository":
    {
        "type": "git",
        "url" : "git+https://github.com/TeaMeow/vuepack.git"
    },
    "keywords":
    [
        "vue",
        "vuex",
        "vue-router",
        "webpack",
        "starter",
        "boilerplate"
    ],
    "author" : "Yami Odymel <yamiodymel@gmail.com>",
    "license": "MIT",
    "bugs":
    {
        "url": "https://github.com/TeaMeow/vuepack/issues"
    },
    "homepage"    : "https://github.com/TeaMeow/vuepack#readme",
    "dependencies":
    {
        "vue"             : "^2.0.0-rc.5",
        "vue-router"      : "^2.0.0-rc.4",
        "vuex"            : "^2.0.0-rc.5",
        "vuex-router-sync": "^3.0.0"
    },
    "devDependencies":
    {
        "autoprefixer"                    : "^6.4.0",
        "babel-loader"                    : "^6.2.4",
        "babel-preset-es2015"             : "^6.13.2",
        "babel-preset-stage-1"            : "^6.13.0",
        "css-loader"                      : "^0.23.1",
        "express"                         : "^4.14.0",
        "extract-text-webpack-plugin"     : "^2.0.0-beta.3",
        "file-loader"                     : "^0.9.0",
        "html-webpack-plugin"             : "^2.22.0",
        "node-sass"                       : "^3.8.0",
        "webpack"                         : "^2.1.0-beta.20",
        "postcss-loader"                  : "^0.9.1",
        "progress-bar-webpack-plugin"     : "^1.9.0",
        "sass-loader"                     : "^4.0.0",
        "style-loader"                    : "^0.13.1",
        "url-loader"                      : "^0.5.7",
        "vue-loader"                      : "^9.4.0",
        "webpack-dev-middleware"          : "^1.6.1",
        "webpack-hot-middleware"          : "^2.12.2",
        "webpack-merge"                   : "^0.8.3"
    }
}
